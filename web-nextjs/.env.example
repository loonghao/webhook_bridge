# Webhook Bridge Next.js Configuration

# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000
NEXT_PUBLIC_WS_BASE_URL=ws://localhost:8000

# Development Configuration
NEXT_PUBLIC_DEV_MODE=true
NEXT_PUBLIC_MOCK_DATA=false

# Build Configuration
NEXT_PUBLIC_BUILD_VERSION=2.0.0-hybrid
NEXT_PUBLIC_BUILD_TIME=

# Feature Flags
NEXT_PUBLIC_ENABLE_WEBSOCKET=true
NEXT_PUBLIC_ENABLE_REAL_TIME=true
NEXT_PUBLIC_ENABLE_DEBUG=false

# Stagewise Debugging Configuration
# Set to 'true' to enable stagewise debugging in production
NEXT_PUBLIC_ENABLE_STAGEWISE=false
# Set to 'true' to enable debug mode (enables more verbose logging)
NEXT_PUBLIC_DEBUG_MODE=false
